{
  "name": "template-cli-monorepo",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "pnpm --filter create-my-template build",
    "dev": "pnpm --parallel --filter @template/server --filter @template/web run dev",
    "dev:server": "pnpm --filter @template/server dev",
    "dev:web": "pnpm --filter @template/web dev",
    "test:templates": "pnpm -r --filter './templates/*' run dev",
    "cli": "node cli/dist/index.js"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@10.25.0"
}
